<listener port="80">
	<!-- localhost -->
	<host name="localhost">
		<context path="/">
			<page location="./intra.ejs" />
		</context>

		<status code="(403|404|500)">
			<page location="./%1$s.ejs" />
		</status>
	</host>
</listener>